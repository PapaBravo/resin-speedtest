{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": "off",
        "indent": [
            "error",
            4
        ],
        "import/no-dynamic-require": 0,
        "no-param-reassign": 0,
        "no-console": 0,
        "no-restricted-syntax": 0,
        "no-await-in-loop": 0,
        "linebreak-style": "off"
    }
}
